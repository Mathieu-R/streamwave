{
  "name": "streamwave",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "build:noprerender": "preact build --no-prerender",
    "build:analyse": "preact build --no-prerender --json=true && webpack-bundle-analyzer stats.json",
    "serve": "preact build && preact serve",
    "serve:noprerender": "npm run build:noprerender && preact serve",
    "dev": "preact watch",
    "lint": "eslint src"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "bundlesize": [
    {
      "path": "./build/bundle.*.js",
      "maxSize": "220kB"
    },
    {
      "path": "./build/route-home.chunk.*.js",
      "maxSize": "220kB"
    }
  ],
  "devDependencies": {
    "copy-webpack-plugin": "^4.5.1",
    "eslint": "^4.19.1",
    "if-env": "^1.0.4",
    "node-sass": "^4.8.3",
    "preact-cli": "^2.2.1",
    "preact-cli-plugin-async": "^1.0.0",
    "preact-cli-plugin-fast-async": "^1.0.1",
    "preact-cli-workbox-plugin": "^1.0.0",
    "sass-loader": "^6.0.7",
    "svg-inline-loader": "^0.8.0"
  },
  "dependencies": {
    "debounce": "^1.1.0",
    "idb-keyval": "^2.5.1",
    "jwt-decode": "^2.2.0",
    "money-clip": "^2.0.1",
    "preact": "^8.2.6",
    "preact-compat": "^3.18.0",
    "preact-router": "^2.5.7",
    "prop-types": "^15.6.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-persist-middleware": "^1.0.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "shaka-player": "^2.3.4",
    "styled-components": "^3.2.3",
    "window-or-global": "^1.0.1",
    "workerize-loader": "^1.0.2"
  }
}
