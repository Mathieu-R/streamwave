{
  "private": true,
  "name": "streamwave",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "build:noprerender": "preact build --no-prerender",
    "serve": "preact build && preact serve",
    "serve:noprerender": "npm run build:noprerender && preact serve",
    "dev": "preact watch",
    "lint": "eslint src"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "bundlesize": [
    {
      "path": "./build/bundle.js",
      "maxSize": "220kB"
    }
  ],
  "devDependencies": {
    "eslint": "^4.16.0",
    "if-env": "^1.0.4",
    "node-sass": "^4.7.2",
    "preact-cli": "^2.0.1",
    "sass-loader": "^6.0.6"
  },
  "dependencies": {
    "idb-keyval": "^2.3.0",
    "jwt-decode": "^2.2.0",
    "preact": "^8.2.6",
    "preact-compat": "^3.18.0",
    "preact-router": "^2.5.7",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.1",
    "shaka-player": "^2.3.2",
    "stockroom": "^1.0.1",
    "unistore": "^3.0.4",
    "workerize-loader": "^1.0.1"
  }
}
